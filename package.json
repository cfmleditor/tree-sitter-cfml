    "cfhtml/queries/*",
    "cfhtml/src/**",
    "cfml/grammar.js",
    "cfml/package.json",
    "cfml/queries/*",
    "cfml/src/**",
    "cfscript/grammar.js",
    "cfscript/package.json",
    "cfscript/queries/*",
    "cfscript/src/**",
    "common/**",
    "*.wasm"
  ],
  "homepage": "https://github.com/cfmleditor/tree-sitter-cfml",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfmleditor/tree-sitter-cfml.git"
  },
  "bugs": {
    "url": "https://github.com/cfmleditor/tree-sitter-cfml/issues"
  },
  "authors": [
    "Gareth Edwards <ghedwards@gmail.com>",
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>",
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.2.2",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint common/define-grammar.js",
    "test": "node --test bindings/node/*_test.js"
