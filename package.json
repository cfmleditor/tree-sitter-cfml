{
  "name": "tree-sitter-cfml",
  "version": "0.20.0",
  "description": "CFML grammar for tree-sitter",
  "main": "bindings/node/index.js",
  "types": "bindings/node",
  "keywords": [
    "parser",
    "lexer"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "homepage": "https://github.com/cfmleditor/tree-sitter-cfml",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfmleditor/tree-sitter-cfml.git"
  },
  "bugs": {
    "url": "https://github.com/cfmleditor/tree-sitter-cfml/issues"
  },
  "authors": [
    "Gareth Edwards <ghedwards@gmail.com>",
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>",
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.21.0",
    "prebuildify": "^6.0.0"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "build-wasm": "tree-sitter build-wasm",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test",
    "test-parse": "tree-sitter parse examples/*.cfm --quiet --time",
    "test-mxunit": "tree-sitter parse test/mxunit/framework/*.cfc --quiet --time",
    "test-windows": "tree-sitter test",
    "build-test": "tree-sitter generate && node-gyp build && tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.cfml",
      "file-types": [
        "cfm",
        "cfc"
      ],
      "injection-regex": "cfml",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "external-files": [
        "src/tag.h"
      ]
    }
  ]
}
