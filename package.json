{
  "name": "@cfmleditor/tree-sitter-cfml",
  "version": "0.4.0",
  "description": "CFML grammar for tree-sitter",
  "repository": "github:cfmleditor/tree-sitter-cfml",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "author": {
    "name": "garetheddies"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "cfml"
  ],
  "files": [
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "cfml/grammar.js",
    "cfml/queries/*",
    "cfml/src/**",
    "cfhtml/grammar.js",
    "cfhtml/queries/*",
    "cfhtml/src/**",
    "cfscript/grammar.js",
    "cfscript/queries/*",
    "cfscript/src/**",
    "common/common.js",
    "common/html_entities.json"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.7"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "node scripts/build.js",
    "parse": "node scripts/parse.js",
    "test": "node scripts/test.js",
    "docswasm": "node scripts/docswasm.js",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "playground": "node scripts/playground.js",
    "testbindings": "node --test bindings/node/*_test.js"
  },
  "publishConfig": {
    "access": "public"
  }
}
