{
  "name": "tree-sitter-cfml",
  "version": "0.20.0",
  "description": "CFML grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer"
  ],
  "homepage": "https://github.com/cfmleditor/tree-sitter-cfml",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfmleditor/tree-sitter-cfml.git"
  },
  "bugs": {
    "url": "https://github.com/cfmleditor/tree-sitter-cfml/issues"
  },
  "authors": [
    "Garet Edwards <ghedwards@gmail.com>",
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>",
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test && tree-sitter parse examples/*.cfm --quiet --time",
    "test-windows": "tree-sitter test",
    "build-test": "tree-sitter generate && node-gyp build && tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "text.cfml.basic",
      "file-types": [
        "cfm",
        "cfc"
      ],
      "injection-regex": "cfml",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ]
    }
  ]
}
